{
    "manifest_version": 2,
    "name": "2read",
    "version": "1.6",
    "homepage_url": "https://2read.net/",
    "description": "Convert article in current tab to readable form and upload it to IPFS.",
    "icons": {
        "16": "icon.svg",
        "24": "icon.svg",
        "32": "icon.svg",
        "64": "icon.svg",
        "128": "icon.svg",
        "256": "icon.svg"
    },
    "permissions": [
        "activeTab",
        "*://*/*",
        "bookmarks"
    ],
    "browser_action": {
        "default_icon": {
            "16": "icon.svg",
            "19": "icon.svg",
            "24": "icon.svg",
            "32": "icon.svg",
            "38": "icon.svg"
        },
        "default_title": "2read"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}
